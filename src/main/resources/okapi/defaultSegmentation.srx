</rule>
<!--footnote references 2-->
<rule break="yes">
<beforebreak>\p{Ll}{2,}[\p{Pe}\p{Pf}"']{0,3}[.!?][0-9]{1,2}</beforebreak>
<afterbreak>\s\s?\p{Lu}</afterbreak>
</rule>
<!--ellipsis following terminal punctuation-->
<rule break="yes">
<beforebreak>[.?!][\s\p{Pe}\p{Pf}\]\}\)]*</beforebreak>
<afterbreak>\u2026</afterbreak>
</rule>
<!--ellipsis followed by opening bracket-->
<rule break="yes">
<beforebreak>\u2026</beforebreak>
<afterbreak>\s*[\[\{\(]+\s*</afterbreak>
</rule>
<!--ellipsis followed by closing bracket-->
<rule break="yes">
<beforebreak>\u2026\s*[\]\}\)]+</beforebreak>
<afterbreak>\s*</afterbreak>
</rule>
<!--inline scripture references-->
<rule break="yes">
<beforebreak>[.?!]['"\p{Pe}\p{Pf}]*</beforebreak>
<afterbreak>\s*[\p{Pi}\p{Ps}\[\{\(]([1-5] )?\p{Lu}</afterbreak>
</rule>
<!--copyright statement-->
<rule break="yes">
<beforebreak>[.?!][\p{Pe}\p{Pf}"']*\s*</beforebreak>
<afterbreak>©\s*[0-9]{4}</afterbreak>
</rule>

</languagerule>

<languagerule languagerulename="Default">
<!-- (!), (??), [...] etc.. -->
<rule break="no">
<beforebreak>[(\[][?!.]+[)\]]</beforebreak>
<afterbreak/>
</rule>

<!-- always break at colon : ICU4J doesn't break here -->
<rule break="yes">
<beforebreak>\w+[\p{Pe}\p{Po}"]*[:]+[\p{Pe}\p{Po}'"\u00BB\u201D]*</beforebreak>
<afterbreak>\s+\P{Ll}</afterbreak>
</rule>

<rule break="yes">
<beforebreak>\w+[\p{Pe}\p{Po}"]*[.?!]+[\p{Pe}\p{Po}'"\u00BB\u201D]*</beforebreak>
<afterbreak>\s+\P{Ll}</afterbreak>
</rule>

<rule break="yes">
<beforebreak>\w+[\p{Pe}\p{Po}"]*[.?!:]+[\p{Pe}\p{Po}'"\u00BB\u201D]*</beforebreak>
<afterbreak>\p{Lu}[^\p{Lu}]</afterbreak>
</rule>

<!-- stop punctuation followed by whitespace and upper case letter 
<rule break="yes">
<beforebreak>[.?!…]</beforebreak>
<afterbreak>\s+\p{Lu}</afterbreak>
</rule>
 -->

<!--newline
<rule break="yes">
<beforebreak>\n</beforebreak>
<afterbreak/>
</rule>
-->
</languagerule>

</languagerules>

<maprules>
<languagemap languagepattern="[Ee][Nn].*" languagerulename="English"/>
<languagemap languagepattern="[Ee][Ss].*" languagerulename="Spanish"/>
<languagemap languagepattern="[Ff][Rr].*" languagerulename="French"/>
<languagemap languagepattern="[Rr][Uu].*" languagerulename="Russian"/>
<languagemap languagepattern="[Nn][Ll].*" languagerulename="Dutch"/>
<languagemap languagepattern="[Pp][Tt].*" languagerulename="Portuguese"/>
<languagemap languagepattern="[Ii][Tt].*" languagerulename="Italian"/>
<languagemap languagepattern="[Dd][Ee].*" languagerulename="German"/>
<!-- All except the above languages are handled complelety by ICU4J -->
<languagemap languagepattern=".*" languagerulename="Default"/>
</maprules>
